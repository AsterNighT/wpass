    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "debug",
            "cargo": {
                "args": [
                    "build",
                    "--bin=wpass",
                    "--package=wpass"
                ],
                "filter": {
                    "name": "wpass",
                    "kind": "bin"
                }
            },
            "args": ["-d", "-l", "dict.zip"],
            "cwd": "${workspaceFolder}"
        },
    ]
}